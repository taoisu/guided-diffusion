    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Attach using Process Id",
            "type": "python",
            "request": "attach",
            "processId": "${command:pickProcess}",
            "justMyCode": false
        },
        {
            "name": "train sr",
            "type": "python",
            "request": "launch",
            "program": "scripts/super_res_train.py",
            "args": [
                "--data_dir",
                "/home/xiaguo/guided-diffusion/data/Random",
                "--diffusion_steps",
                "1000",
                "--noise_schedule",
                "linear",
                "--attention_resolutions",
                "32,16,8",
                "--large_size",
                "512",
                "--small_size",
                "128",
                "--learn_sigma",
                "True",
                "--num_channels",
                "192",
                "--num_head_channels",
                "64",
                "--num_res_blocks",
                "2",
                "--resblock_updown",
                "True",
                "--use_scale_shift_norm",
                "True",
                "--lr",
                "5e-5",
                "--batch_size",
                "8"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}